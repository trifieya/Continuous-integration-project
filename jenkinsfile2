pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
    environment {
dockerhub=credentials('docker')
}
    stages
    {
       
        stage('Chekout Git')
        {
            steps {
                 echo "Getting Project from Git";
                  git branch: "factureF",
                  url:'https://github.com/trifieya/Continuous-integration-project.git'
                
                 
            }
        }
        stage('Docker build')
        {
            steps {
                 sh 'docker build -t mariam1212121121/projet  .'
            }
        }
        stage('Docker login')
        {
            steps {
                sh 'echo $dockerhub_PSW | docker login -u $dockerhub_USR -p $dockerhub_PSW'
            }    
       
        }
      stage('Push') {

			steps {
				sh 'docker push mariam1212121121/projet'
			}
		}
		

    }    
}
